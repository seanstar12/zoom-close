{
  "manifest_version": 2,

  "name": "Zoom Closer",
  "description": "This extension automatically closes the launced window from Zoom meetings.",
  "version": "1.4",

  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },

  "background": {
    "scripts": [
      "background.js" 
    ] 
  },

  "content_scripts": [
    {
      "matches": [
        "https://zoom.us/postattendee",
        "https://zoom.us/j/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
